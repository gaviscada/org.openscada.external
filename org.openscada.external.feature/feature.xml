<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.openscada.external"
      label="openSCADA Externals Feature"
      version="1.2.0.qualifier"
      provider-name="openSCADA.org">

   <description>
      This is a wrapper feature for all external bundles which are
needed by openSCADA.
   </description>

   <copyright>
      See the specific bundle for its copyright information.
   </copyright>

   <license>
      See the specific bundle for its license information.
   </license>

   <requires>
      <import plugin="ch.qos.logback.classic" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.osgi"/>
      <import plugin="osgi.enterprise"/>
      <import plugin="org.slf4j.api"/>
      <import plugin="org.apache.xerces" version="2.9.0" match="greaterOrEqual"/>
      <import plugin="javax.servlet"/>
   </requires>

   <plugin
         id="org.openscada.external.logback.default"
         download-size="0"
         install-size="0"
         version="0.0.0"
         fragment="true"/>

   <plugin
         id="org.openscada.external.postgresql"
         download-size="0"
         install-size="0"
         version="0.0.0"/>

   <plugin
         id="org.openscada.external.odfdom"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.openscada.external.jcifs"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="bcprov"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.paho.client.mqttv3"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="javax.transaction"
         download-size="0"
         install-size="0"
         version="0.0.0"
         fragment="true"
         unpack="false"/>

   <plugin
         id="org.openscada.external.snmp4j"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
